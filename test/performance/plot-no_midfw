reset

set title "RTT with ping (no middlebox)"
#set terminal png transparent nocrop enhanced size 420,320
set terminal png nocrop enhanced
set output 'rtt-no_midfw.png'

set xrange [0:5]
set yrange [0:1.5]
set xtics nomirror
set ytics nomirror
set xlabel ""
set ylabel "ms"
set xtics ("no HIP" 1, "Kernelmode" 2, "Usermode" 3, "ESP Extension" 4)
set style data boxes
set style fill solid border -1
set boxwidth 0.9
plot 'current_data/no_hipd-with_kernelipsec-no_esp_ext-no_midfw-no_wanem-rtt' using (1):1 title '', '' using (1):1:($1-$2):($1+$2) title '' with yerrorbars, 'current_data/with_hipd-with_kernelipsec-no_esp_ext-no_midfw-no_wanem-rtt' using (2):1 title '', '' using (2):1:($1-$2):($1+$2) title '' with yerrorbars, 'current_data/with_hipd-with_useripsec-no_esp_ext-no_midfw-no_wanem-rtt' using (3):1 title '', '' using (3):1:($1-$2):($1+$2) title '' with yerrorbars, 'current_data/with_hipd-with_useripsec-with_esp_ext-no_midfw-no_wanem-rtt' using (4):1 title '', '' using (4):1:($1-$2):($1+$2) title '' with yerrorbars


reset

set title "TCP throughput with iperf (no middlebox)"
#set terminal png transparent nocrop enhanced size 420,320
set terminal png nocrop enhanced
set output 'tcp-no_midfw.png'

set xrange [0:5]
set yrange [0:100]
set xtics nomirror
set ytics nomirror
set xlabel ""
set ylabel "Mbits/sec"
set xtics ("no HIP" 1, "Kernelmode" 2, "Usermode" 3, "ESP Extension" 4)
set style data boxes
set style fill solid border -1
set boxwidth 0.9
plot 'current_data/no_hipd-with_kernelipsec-no_esp_ext-no_midfw-no_wanem-tcp' using (1):1 title '', '' using (1):1:($1-$2):($1+$2) title '' with yerrorbars, 'current_data/with_hipd-with_kernelipsec-no_esp_ext-no_midfw-no_wanem-tcp' using (2):1 title '', '' using (2):1:($1-$2):($1+$2) title '' with yerrorbars, 'current_data/with_hipd-with_useripsec-no_esp_ext-no_midfw-no_wanem-tcp' using (3):1 title '', '' using (3):1:($1-$2):($1+$2) title '' with yerrorbars, 'current_data/with_hipd-with_useripsec-with_esp_ext-no_midfw-no_wanem-tcp' using (4):1 title '', '' using (4):1:($1-$2):($1+$2) title '' with yerrorbars


reset

